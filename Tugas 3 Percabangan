#include <iostream>
using namespace std;
int main()
{
int x;
int y;
int z;
cout<<" Masukkan bilangan x: ";
cin>> x;
cout<<" Masukkan bilangan y: ";
cin>> y;
z = x + y;
cout<< x << " + " << y << " = " << z <<endl;
if (z>0){
cout<<" Nilai "<<z<<" adalah bilangan positif "<<endl;
} else if (z<0){
cout<<" Nilai "<<z<<" adalah bilangan negatif "<<endl;
}
cout<<" Program ini adalah program berhitung "<<endl;
return 0;
}
